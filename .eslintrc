{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"react-app",
		"prettier",
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["react", "prettier"],
	"rules": {
		"no-console": "warn",
		"import/first": "error",
		"react/prop-types": 0,
		"linebreak-style": ["error", "unix"]
	}
}
